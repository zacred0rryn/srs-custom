{
  "version": 1,
  "rules": [
    {
      "type": "logical",
      "mode": "and",
      "rules": [
        {
          "type": "logical",
          "mode": "or",
          "rules": [
            {
              "process_name": [
                "TeamSpeak",
                "TeamSpeak.exe",
                "ts3client_linux_amd64",
                "ts3client_win64.exe"
              ]
            },
            {
              "package_name": "com.teamspeak.ts3client"
            }
          ]
        },
        {
          "domain_suffix": [
            "teamspeak.com",
            ".myteamspeak.com",
            ".teamspeak-services.com",
            ".tmspk.net"
          ],
          "invert": true
        }
      ]
    }
  ]
}
